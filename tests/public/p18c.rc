/* Check 18 (x18a.rc) */

typedef funcptr : int (int a, int b) MYPTRALIAS;
MYPTRALIAS myPtr1 = NULL, myPtr2 = NULL;

function : int addition(int x, int y) {
    return x + y;
}

function : int subtraction(int x, int y) {
    return x - y;
}

function : int main() {
    if (myPtr1 == NULL) {
        myPtr1 = addition;
    }
    cout << myPtr1(4, 6) << endl;
    myPtr2 = subtraction;
    cout << myPtr2(5, 2) << endl;
    myPtr2 = myPtr1;
    cout << myPtr2(5, 2) << endl;
    myPtr2 = NULL;
    return 0;
}
